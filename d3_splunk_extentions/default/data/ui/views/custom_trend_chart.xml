<dashboard showsource="true" script="autodiscover.js">
  <label>Trend Chart with Event Markers</label>
  <search id="trend_search">
    <query>| inputlookup trendchart</query>
  </search> 

  <row>
    <panel>
      <title>Bytes per Page with Releases over Time</title>
      <html>
            <div id="custom" class="splunk-view" data-require="app/d3_splunk_extentions/components/trendchart/trendchart" data-options="{                         &quot;managerid&quot;: &quot;trend_search&quot;, &quot;height&quot;:&quot;600&quot;, &quot;marker_title&quot;:&quot;label&quot;, &quot;marker_value&quot;:&quot;value&quot;, &quot;ylabel&quot;:&quot;bytes&quot;}">
            </div>

        </html>
    </panel>
  </row>
</dashboard>
